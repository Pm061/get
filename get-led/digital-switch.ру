import RPi.GPIO as GPIO
import time

GPIO.setmode(GPIO.BCM)

led = 16
button = 24

GPIO.setup(led, GPIO.OUT)
GPIO.setup(button, GPIO.IN, pull_up_down=GPIO.PUD_UP)

state = False

while True:
    if GPIO.input(button) == GPIO.LOW:
        state = not state
        GPIO.output(led, state)
        time.sleep(0.3)
